#include <stdio.h>

int main() {
    int num, sum = 0;

    
        printf("Enter a numbers with positive and negetivr and end with 999: ");
     
        while(1) 
        {
        scanf("%d", &num);
        if (num == 999) 
            break;
        if (num > 0) 
            sum =sum+ num;
        
        }

    printf("Sum of positive numbers: %d\n", sum);
}
