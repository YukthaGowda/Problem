#include <stdio.h>

int main() {
    float percentage;

    printf("Enter percentage of marks: ");
    scanf("%f", &percentage);

    if (percentage >= 75) 
        printf("First class with distinction!\n");
     else if (percentage >= 60) 
        printf("first class\n");
     else if (percentage >= 50) 
    printf("second class\n");
        else if (percentage >= 35) 
        printf("third class\n");
     else 
        printf("Fail\n");
    

}
